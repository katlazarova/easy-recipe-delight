{
    "name": "katarinalazarova/pineapple",
    "description": "Pineapple theme composer file",
    "type": "project",
    "authors": [
        {
            "name": "katarinalazarova",
            "email": "katarinaalazarova@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.6",
        "oomphinc/composer-installers-extender": "^1.1",
        "npm-asset/vue": "^2.6",
        "npm-asset/swiper": "^5.2"
    },
    "extra": {
        "enable-patching": true,
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "libraries/{$name}": [
                "type:npm-asset"
            ]
        }
    }
}
