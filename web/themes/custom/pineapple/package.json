{
  "name": "pineapple",
  "version": "1.0.0",
  "description": "custom drupal base theme ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lodash": "npx lodash-cli strict -o ./dist/lodash.js include=$( grep -roh \"_\\.[[:alpha:]]*\" ./assets/js ../../../core/assets/vendor/backbone/backbone.js --include=*.js | sort | uniq | cut -b 3- | tr \"\n\" \",\" )",
    "postinstall": "npm-run-all postinstall:**",
    "postinstall:composerinstall": "composer install",
    "postinstall:lodash": "npm run lodash"
  },
  "author": "katarina lazarova",
  "license": "MIT",
  "devDependencies": {
    "grunt": "^1.0.4",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-sass": "^3.0.2",
    "grunt-sass-globbing": "^1.5.1",
    "load-grunt-tasks": "^5.0.0",
    "node-sass": "^4.12.0"
  },
  "dependencies": {
    "breakpoint-sass": "^2.7.1",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5"
  }
}
